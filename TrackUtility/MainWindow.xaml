<Window x:Class="TrackUtility.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrackUtility"
        mc:Ignorable="d"
        Title="SRFS Track Utility" Height="768" Width="1024">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_FileSystem">
                <MenuItem Header="_Open" Click="FileSystem_Open_MenuItem_Click"/>
            </MenuItem>
        </Menu>
        <Grid>
            <TabControl>
                <TabItem Header="Info">
                    
                </TabItem>
                <TabItem Header="Tracks">
                    
                </TabItem>
            </TabControl>
        </Grid>
    </DockPanel>
</Window>
